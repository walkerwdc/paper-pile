{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": "*.json",
      "rules": {
        "prettier/prettier": ["error", { "parser": "json" }]
      }
    }
  ]
}
